<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="adminPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wypozyczalnia.Controllers.zarzadzajPojazdamiAdminController">
    <children>
        <BorderPane prefHeight="30.0" prefWidth="700.0">
            <top>
                <MenuBar maxWidth="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="800.0" BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="Powrót">
                            <items>
                                <MenuItem fx:id="logout_btn" mnemonicParsing="false" onAction="#logOut" text="Do logowania" />
                                <MenuItem fx:id="menuPracownik_btn" mnemonicParsing="false" onAction="#menuAdmin" text="Panel admina" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <bottom>
                <AnchorPane prefHeight="37.0" prefWidth="700.0" style="-fx-background-color: #6d0303;" BorderPane.alignment="CENTER">
                    <children>
                        <Button layoutX="11.0" layoutY="4.0" mnemonicParsing="false" onAction="#dodajAuto" prefHeight="25.0" prefWidth="162.0" text="Dodaj Pojazd" translateX="7.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="185.0" layoutY="4.0" mnemonicParsing="false" onAction="#modujAuto" prefHeight="25.0" prefWidth="162.0" text="Edytuj Pojazd" translateX="5.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="532.0" layoutY="4.0" mnemonicParsing="false" onAction="#usunAuto" prefHeight="25.0" prefWidth="162.0" text="Usuń Pojazd" translateX="5.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="363.0" layoutY="4.0" mnemonicParsing="false" onAction="#klik" prefHeight="25.0" prefWidth="162.0" text="Uzupełnij">
                            <font>
                                <Font size="13.0" />
                            </font>
                        </Button>
                    </children>
                </AnchorPane>
            </bottom>
        </BorderPane>
        <TextField fx:id="autoMarka" layoutX="20.0" layoutY="87.0" promptText="Marka" />
        <TextField fx:id="autoModel" layoutX="20.0" layoutY="122.0" promptText="Model" />
        <TextField fx:id="autoRodzaj" layoutX="20.0" layoutY="157.0" promptText="Rodzaj" />
        <TextField fx:id="autoPaliwo" layoutX="20.0" layoutY="200.0" promptText="Paliwo" />
        <TextField fx:id="autoPrzebieg" layoutX="20.0" layoutY="242.0" promptText="Przebieg" />
        <TextField fx:id="autoCena" layoutX="20.0" layoutY="282.0" promptText="Cena" />
        <TableView fx:id="tabelka_pojazdy" layoutX="222.0" layoutY="75.0" prefHeight="312.0" prefWidth="549.0">
            <columns>
                <TableColumn fx:id="col_marka" prefWidth="77.0" text="Marka" />
                <TableColumn fx:id="col_model" prefWidth="96.0" text="Model" />
                <TableColumn fx:id="col_rodzaj" prefWidth="87.0" text="Rodzaj" />
                <TableColumn fx:id="col_paliwo" prefWidth="85.0" text="Paliwo" />
                <TableColumn fx:id="col_przebieg" prefWidth="96.0" text="Przebieg" />
                <TableColumn fx:id="col_cena" prefWidth="107.0" text="Cena" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
