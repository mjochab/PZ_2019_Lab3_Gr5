<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="adminPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wypozyczalnia.Controllers.zarzadzajPracownikamiController">
    <children>
        <BorderPane prefHeight="30.0" prefWidth="700.0">
            <top>
                <MenuBar maxWidth="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="800.0" BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="Powrót">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#logOut" text="Do logowania" />
                                <MenuItem fx:id="menuPracownik_btn" mnemonicParsing="false" onAction="#menuAdmin" text="Panel admina" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <bottom>
                <AnchorPane prefHeight="37.0" prefWidth="700.0" style="-fx-background-color: #6d0303;" BorderPane.alignment="CENTER">
                    <children>
                        <Button layoutX="8.0" layoutY="3.0" mnemonicParsing="false" onAction="#dodajPracownika" prefHeight="25.0" prefWidth="162.0" text="Dodaj Pracownika" translateX="7.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="182.0" layoutY="3.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="162.0" text="Edytuj Pracownika" translateX="5.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="353.0" layoutY="3.0" mnemonicParsing="false" onAction="#klik" prefHeight="25.0" prefWidth="162.0" text="Uzupełnij" translateX="5.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                  <Button layoutX="529.0" layoutY="3.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="162.0" text="Usuń Pracownika">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>

                    </children>
                </AnchorPane>
            </bottom>
        </BorderPane>
        <TextField fx:id="imiePracownik" layoutX="20.0" layoutY="89.0" promptText="Imie" />
        <TextField fx:id="nazwiskoPracownik" layoutX="20.0" layoutY="130.0" promptText="Nazwisko" />
        <TextField fx:id="loginPracownik" layoutX="20.0" layoutY="169.0" promptText="Login" />
        <TextField fx:id="peselPracownik" layoutX="20.0" layoutY="210.0" promptText="Pesel" />
        <TableView fx:id="tabelka_pracownicy" layoutX="224.0" layoutY="79.0" prefHeight="292.0" prefWidth="560.0">
            <columns>
                <TableColumn fx:id="col_imie" prefWidth="117.0" text="Imie" />
                <TableColumn fx:id="col_nazwisko" prefWidth="134.0" text="Nazwisko" />
                <TableColumn fx:id="col_login" prefWidth="160.0" text="Login" />
                <TableColumn fx:id="col_pesel" prefWidth="148.0" text="Pesel" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
