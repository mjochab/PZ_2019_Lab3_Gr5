<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane fx:id="adminPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.zarzadzajPojazdamiAdminController">
    <children>
        <BorderPane prefHeight="30.0" prefWidth="700.0">
            <top>
                <MenuBar prefHeight="25.0" prefWidth="700.0" BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="Powrót">
                            <items>
                                <MenuItem fx:id="logout_btn" mnemonicParsing="false" onAction="#logOut" text="Do logowania" />
                                <MenuItem fx:id="menuPracownik_btn" mnemonicParsing="false" onAction="#menuAdmin" text="Panel admina" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <bottom>
                <AnchorPane prefHeight="37.0" prefWidth="700.0" style="-fx-background-color: #6d0303;" BorderPane.alignment="CENTER">
                    <children>
                        <Button layoutY="4.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="162.0" text="Dodaj Pojazd" translateX="7.0" fx:id="button" onAction="#DodajAuto"/>
                        <Button layoutX="174.0" layoutY="4.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="162.0" text="Edytuj Pojazd" translateX="5.0" />
                        <Button layoutX="345.0" layoutY="4.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="162.0" text="Usuń Pojazd" translateX="5.0" />
                    </children>
                </AnchorPane>
            </bottom>
        </BorderPane>
        <TextField layoutX="28.0" layoutY="78.0" promptText="Marka" fx:id="autoMarka"/>
        <TextField layoutX="28.0" layoutY="112.0" promptText="Model" fx:id="autoModel"/>
        <TextField layoutX="28.0" layoutY="144.0" promptText="Rodzaj" fx:id="autoRodzaj"/>
        <TextField layoutX="28.0" layoutY="177.0" promptText="Rocznik" fx:id="autoRocznik"/>
        <TextField layoutX="28.0" layoutY="212.0" promptText="Paliwo" fx:id="autoPaliwo"/>
        <TextField layoutX="28.0" layoutY="249.0" promptText="Przebieg" fx:id="autoPrzebieg"/>
        <TextField layoutX="28.0" layoutY="284.0" promptText="Cena" fx:id="autoCena"/>
        <TableView fx:id="tabelka_pojazdy" layoutX="222.0" layoutY="75.0" prefHeight="312.0" prefWidth="464.0">
            <columns>
                <TableColumn fx:id="col_marka" prefWidth="77.0" text="Marka" />
                <TableColumn fx:id="col_model" prefWidth="96.0" text="Model" />
                <TableColumn fx:id="col_rodzaj" prefWidth="87.0" text="Rodzaj" />
                <TableColumn fx:id="col_paliwo" prefWidth="63.0" text="Paliwo" />
                <TableColumn fx:id="col_przebieg" prefWidth="83.0" text="Przebieg" />
                <TableColumn fx:id="col_cena" prefWidth="57.0" text="Cena" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
