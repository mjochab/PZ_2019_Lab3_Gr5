<AnchorPane fx:id="pracownikPane" prefHeight="900.0" prefWidth="1440.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wypozyczalnia.Controllers.udostepnijPojazdController">
    <children>
        <BorderPane prefHeight="30.0" prefWidth="700.0">
            <top>
                <MenuBar maxWidth="-Infinity" minWidth="-Infinity" prefHeight="32.0" prefWidth="891.0" BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="Powrót">
                            <items>
                                <MenuItem fx:id="logout_btn" mnemonicParsing="false" onAction="#logOut" text="Do logowania" />
                                <MenuItem fx:id="menuPracownik_btn" mnemonicParsing="false" onAction="#menuPracownik" text="Panel pracownik" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <bottom>
                <AnchorPane prefHeight="37.0" prefWidth="700.0" style="-fx-background-color: #6d0303;" BorderPane.alignment="CENTER">
               <children>
                        <Button layoutX="22.0" layoutY="4.0" mnemonicParsing="false" onAction="#dodajAuto" prefHeight="25.0" prefWidth="149.0" text="Dodaj Pojazd">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                  <Button layoutX="347.0" layoutY="4.0" mnemonicParsing="false" onAction="#klik" prefHeight="25.0" prefWidth="149.0" text="Uzupełnij">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                  <Button layoutX="183.0" layoutY="4.0" mnemonicParsing="false" onAction="#modujAuto" prefHeight="25.0" prefWidth="149.0" text="Edytuj Pojazd">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                  <Button layoutX="505.0" layoutY="4.0" mnemonicParsing="false" onAction="#usunAuto" prefHeight="25.0" prefWidth="149.0" text="Usuń Pojazd">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
               </children>
                </AnchorPane>
            </bottom>
        </BorderPane>
        <TextField fx:id="autoMarka" layoutX="14.0" layoutY="79.0" prefHeight="31.0" prefWidth="151.0" promptText="Marka" />
        <TextField fx:id="autoModel" layoutX="14.0" layoutY="120.0" prefHeight="31.0" prefWidth="151.0" promptText="Model" />
        <TextField fx:id="autoRodzaj" layoutX="13.0" layoutY="158.0" prefHeight="31.0" prefWidth="151.0" promptText="Rodzaj" />
        <TextField fx:id="autoRocznik" layoutX="13.0" layoutY="193.0" prefHeight="31.0" prefWidth="151.0" promptText="Rocznik" />
        <TextField fx:id="autoPaliwo" layoutX="13.0" layoutY="228.0" prefHeight="31.0" prefWidth="151.0" promptText="Paliwo" />
        <TextField fx:id="autoPrzebieg" layoutX="13.0" layoutY="263.0" prefHeight="31.0" prefWidth="151.0" promptText="Przebieg" />
        <TextField fx:id="autoCena" layoutX="13.0" layoutY="298.0" prefHeight="31.0" prefWidth="151.0" promptText="Cena" />
        <TextField fx:id="autoDostep" layoutX="13.0" layoutY="336.0" prefHeight="31.0" prefWidth="151.0" promptText="Dostęp" />
        <TableView fx:id="tabelka_pojazdy" layoutX="275.0" layoutY="83.0" prefHeight="291.0" prefWidth="665.0">
            <columns>
                <TableColumn fx:id="col_marka" prefWidth="84.0" text="Marka" />
                <TableColumn fx:id="col_model" prefWidth="64.0" text="Model" />
                <TableColumn fx:id="col_rodzaj" prefWidth="61.0" text="Rodzaj" />
                <TableColumn fx:id="col_rocznik" prefWidth="100.0" text="Rocznik" />
                <TableColumn fx:id="col_paliwo" minWidth="0.0" prefWidth="76.0" text="Paliwo" />
                <TableColumn fx:id="col_przebieg" prefWidth="109.0" text="Przebieg" />
                <TableColumn fx:id="col_cena" prefWidth="75.0" text="Cena" />
                <TableColumn fx:id="col_dostep" prefWidth="95.0" text="Dostęp" />

            </columns>
        </TableView>
    </children>
</AnchorPane>
