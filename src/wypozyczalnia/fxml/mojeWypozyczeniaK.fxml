<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="klientPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wypozyczalnia.Controllers.MenuKlientController">
    <children>
        <BorderPane maxWidth="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="800.0">
            <top>
                <MenuBar prefHeight="25.0" prefWidth="700.0" BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="Powrót">
                            <items>
                                <MenuItem fx:id="logout_btn" mnemonicParsing="false" onAction="#logOut" text="Do logowania" />
                                <MenuItem fx:id="menuPracownik_btn" mnemonicParsing="false" onAction="#menuKlient" text="Panel klienta" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <bottom>
                <AnchorPane prefHeight="37.0" prefWidth="700.0" style="-fx-background-color: #6d0303;" stylesheets="@../../../style.css" BorderPane.alignment="CENTER">
                    <children>
                        <Button layoutX="13.0" layoutY="4.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="139.0" text="Edytuj Wypozyczenie" translateX="7.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="174.0" layoutY="4.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="118.0" text="Potwierdź" translateX="5.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                    </children>
                </AnchorPane>
            </bottom>
        </BorderPane>
        <TextField layoutX="16.0" layoutY="92.0" prefHeight="31.0" prefWidth="132.0" promptText="Marka" />
        <TextField layoutX="16.0" layoutY="127.0" prefHeight="31.0" prefWidth="132.0" promptText="Model" />
        <TextField layoutX="16.0" layoutY="162.0" prefHeight="31.0" prefWidth="132.0" promptText="Data początkowa" />
        <TextField layoutX="16.0" layoutY="197.0" prefHeight="31.0" prefWidth="132.0" promptText="Data końcowa" />
        <TableView layoutX="178.0" layoutY="77.0" prefHeight="294.0" prefWidth="608.0">
            <columns>
                <TableColumn fx:id="col_marka" prefWidth="120.0" text="Marka" />
                <TableColumn fx:id="col_model" prefWidth="73.0" text="Model" />
                <TableColumn fx:id="col_dataStart" prefWidth="165.0" text="Data początkowa" />
                <TableColumn fx:id="col_dataStop" prefWidth="133.0" text="Data końcowa" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
