<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pracownikPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wypozyczalnia.Controllers.zarzadzajPojazdamiController">
    <children>
        <BorderPane prefHeight="30.0" prefWidth="700.0">
            <top>
                <MenuBar prefHeight="25.0" prefWidth="700.0" BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="Powrót">
                            <items>
                                <MenuItem fx:id="logout_btn" mnemonicParsing="false" onAction="#logOut" text="Do logowania" />
                                <MenuItem fx:id="menuPracownik_btn" mnemonicParsing="false" onAction="#menuPracownik" text="Panel pracownik" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <bottom>
                <AnchorPane prefHeight="37.0" prefWidth="700.0" style="-fx-background-color: #6d0303;" BorderPane.alignment="CENTER">
                    <children>
                        <Button layoutX="16.0" layoutY="4.0" mnemonicParsing="false" onAction="#dodajAuto" prefHeight="25.0" prefWidth="162.0" text="Dodaj Pojazd" translateX="7.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="190.0" layoutY="4.0" mnemonicParsing="false" onAction="#modujAuto" prefHeight="25.0" prefWidth="162.0" text="Edytuj Pojazd" translateX="5.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="361.0" layoutY="4.0" mnemonicParsing="false" onAction="#usunAuto" prefHeight="25.0" prefWidth="162.0" text="Usuń Pojazd" translateX="5.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                  <Button layoutX="537.0" layoutY="4.0" mnemonicParsing="false" onAction="#klik" prefHeight="29.0" prefWidth="89.0" text="Uzupełnij">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
                    </children>
                </AnchorPane>
            </bottom>
        </BorderPane>
        <TextField fx:id="autoMarka" layoutX="20.0" layoutY="88.0" promptText="Marka" />
        <TextField fx:id="autoModel" layoutX="20.0" layoutY="123.0" promptText="Model" />
        <TextField fx:id="autoRodzaj" layoutX="20.0" layoutY="158.0" promptText="Rodzaj" />
        <TextField fx:id="autoRocznik" layoutX="20.0" layoutY="193.0" promptText="Rocznik" />
        <TextField fx:id="autoPaliwo" layoutX="20.0" layoutY="228.0" promptText="Paliwo" />
        <TextField fx:id="autoPrzebieg" layoutX="20.0" layoutY="263.0" promptText="Przebieg" />
        <TextField fx:id="autoCena" layoutX="20.0" layoutY="298.0" promptText="Cena" />
        <TableView fx:id="tabelka_pojazdy" layoutX="227.0" layoutY="73.0" prefHeight="314.0" prefWidth="459.0">
            <columns>
                <TableColumn fx:id="col_marka" prefWidth="67.0" text="Marka" />
                <TableColumn fx:id="col_model" prefWidth="66.0" text="Model" />
                <TableColumn fx:id="col_rodzaj" prefWidth="84.0" text="Rodzaj" />
                <TableColumn fx:id="col_paliwo" prefWidth="91.0" text="Paliwo" />
                <TableColumn fx:id="col_przebieg" prefWidth="90.0" text="Przebieg" />
                <TableColumn fx:id="col_cena" prefWidth="60.0" text="Cena" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
