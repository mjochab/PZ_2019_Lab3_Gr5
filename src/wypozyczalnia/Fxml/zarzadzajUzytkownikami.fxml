<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pracownikPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wypozyczalnia.Controllers.zarzadzajUzytkownikamiController">
    <children>
        <BorderPane prefHeight="30.0" prefWidth="700.0">
            <top>
                <MenuBar maxWidth="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="800.0" BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="Powrót">
                            <items>
                                <MenuItem fx:id="logout_btn" mnemonicParsing="false" onAction="#logOut" text="Do logowania" />
                                <MenuItem fx:id="menuPracownik_btn" mnemonicParsing="false" onAction="#menuPracownik" text="Panel pracownik" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <bottom>
                <AnchorPane prefHeight="37.0" prefWidth="700.0" style="-fx-background-color: #6d0303;" BorderPane.alignment="CENTER">
                    <children>
                        <Button layoutX="5.0" layoutY="3.0" mnemonicParsing="false" onAction="#dodajUsera" prefHeight="25.0" prefWidth="162.0" text="Dodaj Uzytkownika" translateX="7.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="179.0" layoutY="3.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="162.0" text="Edytuj Uzytkownika" translateX="5.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                        <Button layoutX="350.0" layoutY="3.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="162.0" text="Usuń Uzytkownika" translateX="5.0">
                     <font>
                        <Font size="13.0" />
                     </font></Button>
                    </children>
                </AnchorPane>
            </bottom>
        </BorderPane>
      <TextField fx:id="userImie" layoutX="16.0" layoutY="96.0" promptText="Imie" />
      <TextField fx:id="userNazwisko" layoutX="16.0" layoutY="131.0" promptText="Nazwisko" />
      <TextField fx:id="userData" layoutX="16.0" layoutY="166.0" promptText="Data Urodzenia" />
      <TextField fx:id="userMiejscowosc" layoutX="16.0" layoutY="201.0" promptText="Miejscowość" />
      <TextField fx:id="userTelefon" layoutX="16.0" layoutY="236.0" promptText="Numer Telefonu" />
      <TextField fx:id="userMail" layoutX="16.0" layoutY="271.0" promptText="Email" />
      <TextField fx:id="userPesel" layoutX="16.0" layoutY="306.0" promptText="Pesel" />
      <TableView fx:id="tabelka" layoutX="226.0" layoutY="73.0" prefHeight="298.0" prefWidth="451.0">
          <columns>
              <TableColumn fx:id="col_imie" prefWidth="46.0" text="imie" />
              <TableColumn fx:id="col_nazwisko" prefWidth="117.0" text="nazwisko" />
              <TableColumn fx:id="col_data" prefWidth="81.0" text="data_urodzenia" />
              <TableColumn fx:id="col_miejscowosc" prefWidth="124.0" text="miejscowosc" />
              <TableColumn fx:id="col_pesel" prefWidth="82.0" text="pesel" />
          </columns>
      </TableView>
    </children>
</AnchorPane>
